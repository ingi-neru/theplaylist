<!DOCTYPE html>
<html>
  <%- include('partials/head', { title }) %>
  <body>
    <%- include('partials/search') %>
        <% artists.forEach(function(artist) { %>
            <div class="artist">
                <h2><%= artist.name %></h2>
                <ul>
                    <% artist.albums.forEach(function(album) { %>
                        <li><%= album.title %></li>
                    <% }); %>
                </ul>
        <% }); %>
  </body>
</html>
